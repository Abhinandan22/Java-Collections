import java.util.*;

class Arrays1D {

	private static int[] arrays1D(int n) {
		
		int arr[] = new int[n];
		if(n>=0 && n<=100){
		
		int value = 0;

		for(int i = 0 ; i<=n-1 ; i++){
			arr[i] = value;
			value+=2;
		}
		
		}
		return arr;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] ans =arrays1D(n);
		
		for(int x:ans)
			System.out.print(x+" "); 
	}
}
